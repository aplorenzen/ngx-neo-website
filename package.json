{
  "name": "ngx-neo-website",
  "version": "0.4.0",
  "license": "MIT",
  "private": true,
  "config": {
    "docker-namespace": "aplorenzen"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod && ng run neo-website:pre-render",
    "postbuild": "npm run render",
    "postinstall": "npm run compile_ng-bootstrap && npm run compile_ng-scrollreveal",
    "render": "ts-node prerender.ts",
    "build:ci": "npm run env -s && ng build --prod --build-optimizer --vendor-chunk && ng run neo-website:pre-render && npm run postbuild",
    "compile_ng-bootstrap": "./node_modules/.bin/babel node_modules/@ng-bootstrap/ng-bootstrap -d node_modules/@ng-bootstrap/ng-bootstrap --presets es2015",
    "compile_ng-scrollreveal": "./node_modules/.bin/babel node_modules/ng-scrollreveal -d node_modules/ng-scrollreveal --presets es2015",
    "jenkins:buildinfo": "node replace.buildinfo.js",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint neo-website --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "docker:build": "bash -c ./get_docker_image_name.sh | xargs -0 -I % docker build -t % dist/",
    "docker:push": "bash -c ./get_docker_image_name.sh | xargs -0 -I % docker push %"
  },
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@ngx-translate/core": "^9.1.1",
    "@nicky-lenaers/ngx-scroll-to": "^0.6.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.7",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "ng-scrollreveal": "^2.2.0",
    "popper.js": "^1.14.3",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "scrollreveal": "^3.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@angular/platform-server": "6.0.7",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.2.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.110",
    "@types/node": "^8.10.20",
    "@types/scrollreveal": "0.0.3",
    "codelyzer": "^4.4.2",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.3.2",
    "puppeteer": "^1.5.0",
    "replace-in-file": "^3.4.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "~3.1.0",
    "stylelint-config-standard": "~18.1.0",
    "stylelint-scss": "~2.4.0",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
