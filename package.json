{
  "name": "ngx-neo-website",
  "version": "0.3.4",
  "license": "MIT",
  "private": true,
  "config": {
    "docker-namespace": "aplorenzen"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build:ci": "npm run env -s && ng build --prod --build-optimizer --vendor-chunk",
    "jenkins:buildinfo": "node replace.buildinfo.js",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "docker:build": "bash -c ./get_docker_image_name.sh | xargs -0 -I % docker build -t % dist/",
    "docker:push": "bash -c ./get_docker_image_name.sh | xargs -0 -I % docker push %"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@ngx-translate/core": "^9.1.1",
    "@nicky-lenaers/ngx-scroll-to": "^0.6.1",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "ng-scrollreveal": "^2.2.0",
    "pace-progress": "^1.0.2",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.5",
    "scrollreveal": "^3.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.2.0",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.15",
    "@types/scrollreveal": "0.0.3",
    "codelyzer": "^4.3.0",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.3.2",
    "puppeteer": "^1.4.0",
    "replace-in-file": "^3.4.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "~3.1.0",
    "stylelint-config-standard": "~18.1.0",
    "stylelint-scss": "~2.4.0",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2"
  }
}
